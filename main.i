                paddle.paddle_col = paddle.paddle_col-2;
            }
        }

    if((~(*(volatile unsigned int *)0x04000130) & ((1<<4))))
        {
            if(paddle.paddle_col<(240-paddle.paddle_sizel))
            {

                paddle.paddle_col = paddle.paddle_col+2;
            }
        }


    if(ball.row < paddle.paddle_sizew)
    {
        if(((ball.col+ball.size)>paddle.paddle_col) && (ball.col<(paddle.paddle_col+paddle.paddle_sizel)))
        {
            ball.row= paddle.paddle_sizew;
            ball.rd=-ball.rd;
        }
        if(ball.row<=0)
        {
            paddle.dead=1;
        }
    }


}




void draw()
{

    waitForVblank();
    drawRect(ball.oldrow, ball.oldcol, ball.size, ball.size, 0);
    drawRect(ball.row, ball.col, ball.size, ball.size, ((31) | (31)<<5 | (0)<<10));


    waitForVblank();
    drawRect(paddle.paddle_row_old,paddle.paddle_col_old, paddle.paddle_sizew, paddle.paddle_sizel, 0);
    drawRect(paddle.paddle_row, paddle.paddle_col, paddle.paddle_sizew, paddle.paddle_sizel, ((0) | (31)<<5 | (0)<<10));


    waitForVblank();
    for(int i = 0; i < brickCount; i++)
    {
        BRICK * b = &bricks[i];
        drawBricks(b);
    }
    for(int i = 0; i < brickCount; i++)
    {
        BRICK * b = &bricks[i];
        eraseBricks(b);
    }


    waitForVblank();
    char str[80];
        sprintf(str, "Score: %u", score);
        drawString(2, 0, str, ((31) | (31)<<5 | (31)<<10));

}

void eraseBricks(BRICK* b)
{
    if((b->active == 0)&&(b->tobeerased))
        drawRect(b->oldRow, b->oldCol, b->height, b->width, 0);
        b->tobeerased = 0;
}

void drawBricks(BRICK* b)
{
    if(b->active)
        drawRect(b->row, b->col, b->height, b->width, b->color);
}

void eraseScore()
{
    char strover[80];
    sprintf(strover, "Score: %u", oldScore);
    drawString(2, 0, strover, 0);
}

